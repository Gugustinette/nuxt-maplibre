{
  "name": "nuxt-maplibre",
  "version": "1.2.0",
  "description": "A Nuxt module to use MapLibre",
  "homepage": "https://gugustinette.github.io/nuxt-maplibre",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gugustinette/nuxt-maplibre.git"
  },
  "bugs": {
    "url": "https://github.com/Gugustinette/nuxt-maplibre/issues"
  },
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:host": "nuxi dev playground --host",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@indoorequal/vue-maplibre-gl": "^8.4.2",
    "maplibre-gl": "^5.15.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.0.1",
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxt/kit": "^4.2.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.0",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxt/ui": "^2.18.2",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "caniuse-lite": "^1.0.30001754",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.0",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4"
  }
}
